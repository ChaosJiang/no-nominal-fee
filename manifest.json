{
    "name": "指名料なし",
    "description": "料金なくでもスタイリストを予約できるように!",
    "version": "1.0",
    "manifest_version": 2,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["storage", "tabs", "https://beauty.hotpepper.jp/CSP/bt/reserve/schedule*"],
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "/images/get_started16.png",
            "32": "/images/get_started32.png",
            "48": "/images/get_started48.png",
            "128": "/images/get_started128.png"
        }
    },
    "icons": {
        "16": "/images/get_started16.png",
        "32": "/images/get_started32.png",
        "48": "/images/get_started48.png",
        "128": "/images/get_started128.png"
    },
    "options_page": "options.html",
    "content_scripts": [
        {
            "matches": ["https://beauty.hotpepper.jp/CSP/bt/reserve/*"],
            "exclude_matches": ["https://beauty.hotpepper.jp/CSP/bt/reserve/schedule*"],
            "js": ["content.js"],
            "run_at": "document_end"
        }
    ]
}